-- ASSUME ENOUGH REGISTERS FOR INPUTS
    INBOX
START:
    COPYTO [14]
    OUTBOX
    BUMPUP 14 --WHERE NEXT VALUE SHOULD BE STORED
DISCARDSTART:
    COPYFROM 14
    COPYTO 13 --CURRENT SEARCH POSITION
    BUMPDN 13
    INBOX
    COPYTO 12 --CURRENT SEARCH STRING
SEARCH:
    COPYFROM [13]
    SUB 12
    JUMPZ DISCARDSTART
    BUMPDN 13
    JUMPN END
    JUMP SEARCH
END:
    COPYFROM 12
    JUMP START